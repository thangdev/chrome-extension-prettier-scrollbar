{
  "name": "Scroll bar editor",
  "description": "This extension can customize the style of scroll bar in webpage.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [],
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"],
      "css": ["my.css"]
    }
  ]
}


